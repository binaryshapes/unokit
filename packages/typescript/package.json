{
  "name": "@nuxo/typescript",
  "version": "1.0.0",
  "description": "Typescript configurations for Nuxo",
  "author": "Carlos Cosming",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/binaryshapes/unokit.git",
    "directory": "packages/typescript"
  },
  "keywords": [
    "typescript",
    "tsconfig"
  ],
  "files": [
    "*.json"
  ],
  "exports": {
    "./*.json": "./*.json"
  },
  "scripts": {
    "publish-local": "pnpm publish --registry=http://localhost:4873 --no-git-checks",
    "unpublish-local": "npm unpublish --registry=http://localhost:4873/ --force",
    "republish-local": "pnpm unpublish-local && pnpm publish-local"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "vite": "^7.0.4"
  },
  "peerDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vitest": "^3.1.4"
  }
}
