{
  "extends": "./base.json",
  "compilerOptions": {
    "emitDeclarationOnly": false,
    "forceConsistentCasingInFileNames": true,
    "types": ["node"],
    "strictNullChecks": true,
    "noImplicitAny": true,
    "strictBindCallApply": true,
    "tsBuildInfoFile": "dist/tsconfig.lib.tsbuildinfo",
    "declarationMap": false
  },
  "exclude": [
    "${configDir}/tests/**/*.ts",
    "${configDir}/examples/**/*.ts"
  ],
  "references": []
}
